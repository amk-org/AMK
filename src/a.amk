import logics.proposition

theorem belong1:
	require:
		define a of statement
		define b of statement
	conclude:
		a, b |- a

theorem belong:
	require:
		define a of statement
		define b of set[statement] 
	conclude:
		a,b|-a

theorem zhqc:
	require:
		define a of statement
		define b of statement
	conclude:
		a vee b vee not b
		a wedge b |-| c
	proof:
		a |- b -[theorem t: <1> <2>] <1>
		a -> b <2>
		b |- a -[: <2>] <t3>
		bs |- nsk -[t] <r>
		c

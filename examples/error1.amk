import logics.proposition

theorem belong:
	require:
		define a of statement
		define b of set[statement] 
	conclude:
		a,b|-a

theorem belong2:
	require:
		define a of statement
		define b of statement
		define c of statement
		define d of statement
	conclude:
		a,b,c|-b
	proof:
		a,b,c|-a -[belong1] <1>
		a,b,c|-c -[belong] <2>
		a,b,c|-b -[belong] <2>
